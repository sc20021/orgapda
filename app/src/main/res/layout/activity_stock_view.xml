        android:ems="10"
        android:inputType="textPersonName"
        android:nextFocusDown="@+id/storkfake1"
        android:textColor="#000000"
        android:paddingLeft="5dp"
        app:layout_constraintBottom_toTopOf="@+id/storkfake1"
